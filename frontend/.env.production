# Production Environment Variables
NODE_ENV=production
REACT_APP_ENV=production
GENERATE_SOURCEMAP=false

# API Configuration
REACT_APP_API_URL=/api
REACT_APP_GA_TRACKING_ID=

# Performance Settings
REACT_APP_LAZY_LOAD=true
REACT_APP_IMAGE_QUALITY=75

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false

# Cache Settings
REACT_APP_CACHE_TTL=3600

# Security Headers
REACT_APP_CSP="default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self' data:;"
